{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom","dom.iterable","esnext"],
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "types": ["webpack-env"],
		"skipLibCheck": false
  },
  "include": ["src"]
}
